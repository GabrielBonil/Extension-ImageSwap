{
    "manifest_version": 3,
    "name": "ImageSwap",
    "description": "Extension to swap all the images on the screen to the image provided in the input",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "action": {
      "default_popup": "index.html"
    }
  }